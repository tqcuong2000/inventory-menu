{
  "schemaVersion": 1,
  "id": "inventory-menu",
  "version": "${version}",
  "name": "Inventory Menu",
  "description": "Inventory Menu is a server-side mod designed to allows player and datapack maker. The mod's core feature allows you to create fully custom chest menus simply by writing simple JSON files!",
  "authors": [
    "InfinityI"
  ],
  "contact": {},
  "license": "LGPL-3.0",
  "icon": "assets/inventory-menu/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "infinityi.inventorymenu.InventoryMenu"
    ],
    "client": [
      "infinityi.inventorymenu.InventoryMenuClient"
    ]
  },
  "mixins": [
    "inventory-menu.mixins.json",
    {
      "config": "inventory-menu.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
